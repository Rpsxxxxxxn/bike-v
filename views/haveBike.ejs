<a href="/bike/list">所有車体の追加</a>
<div class="mx-auto my-5 px-3" style="max-width: 700px">
  <% haveBikeList.forEach((data)=> { %>
    <div class="card mb-3">
      <div class="card-body">
        <h5 class="card-title">
          <%= data.company %>
            <%= data.name %>
        </h5>

        <ul class="list-group list-group-flush">
          <li class="list-group-item">オイル交換時 <%= data.oil_change %>L</li>
          <li class="list-group-item">フィルタ交換時 <%= data.oil_filter %>L</li>
          <li class="list-group-item">モデル <%= data.model %>
          </li>
          <li class="list-group-item">排気量 <%= data.cc %>cc</li>
          <li class="list-group-item"><a href='<%= data.body_link %>' target="_blank" rel="noopener noreferrer">車体公式サイト</a></li>
        </ul>
      </div>
      <div class="card-footer">
        <a href="/bike/have/delete/<%= data.id %>" class="btn btn-danger">所有している車体の削除</a>
      </div>
    </div>
  <% }) %>
</div>